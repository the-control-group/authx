  "dependencies": {},
  "description": "",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "ava": "^2.3.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "!dist/test.*"
  ],
  "keywords": [
    "oauth",
    "aim",
    "acl",
    "access control",
    "permissions"
  ],
  "license": "MIT",
  "main": "dist",
  "name": "@authx/manager",
  "peerDependencies": {},
  "repository": "https://github.com/the-control-group/authx",
  "scripts": {
    "format": "prettier --list-different --write '**/*.{json,yml,md,ts}'",
    "lint": "prettier -c '**/*.{json,yml,md,ts}' && eslint src --ext ts",
    "build": "rm -rf dist && tsc",
    "build:development": "rm -rf dist && tsc --watch",
    "start": "NODE_ENV=production node dist/server",
    "start:development": "NODE_ENV=development nodemon --delay 2 --inspect dist/server",
    "test": "ava --verbose dist/*.test.js",
    "test:development": "ava --verbose --watch dist/*.test.js"
  },
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
